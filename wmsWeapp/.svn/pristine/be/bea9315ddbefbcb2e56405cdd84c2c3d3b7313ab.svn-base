import { defineStore } from 'pinia'
import Taro from '@tarojs/taro'

export const useTabBarStore = defineStore('tabBar', {
  state: () => {
    return { 
      tabbarActive: 0
    }
  },
  getters: {
  },
  actions: {
    setTabbarActive(index) {
      this.tabbarActive = index
    }
  },
})
